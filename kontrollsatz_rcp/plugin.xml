<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.ametras.kontrollsatz.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.ametras.kontrollsatz.Perspective"
            name="RCP Perspective"
            id="com.ametras.kontrollsatz.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.ametras.controlrecord.view.FilesData"
            id="com.ametras.controlrecord.FilesData"
            name="FilesData"
            restorable="true">
      </view>
   </extension>

</plugin>
